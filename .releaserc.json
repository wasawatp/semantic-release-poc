{
  "branches": [
    {
      "name":"+([0-9])?(.{+([0-9]),x}).x"
    },
    {
      "name": "main"
    },
    {
      "name": "next"
    },
    {
      "name": "next-major"
    },
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    ["@semantic-release/exec", {
      "prepareCmd" : "set-version ${nextRelease.version}",
      "publishCmd" : "publish-package"
      }]
  ]
}